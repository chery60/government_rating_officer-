<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.governmentofficerrating"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rate_us"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.governmentofficerrating.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.governmentofficerrating.Registration"
            android:label="@string/title_activity_registration" >
        </activity>
        <activity
            android:name="com.example.governmentofficerrating.ListOfGovernmentOffice"
            android:label="@string/title_activity_list_of_government_office" >
        </activity>
        <activity
            android:name="com.example.governmentofficerrating.EmloyeeDetails"
            android:label="@string/title_activity_emloyee_details" >
        </activity>
        <activity
            android:name="com.example.governmentofficerrating.RatingEmployee"
            android:label="@string/title_activity_rating_employee" >
        </activity>
        <activity
            android:name="com.example.governmentofficerrating.Splash"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>